<?xml version="1.0" encoding="utf-8"?><!--订单列表-->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/dp_10"
    android:background="@color/color_white"
    android:paddingLeft="@dimen/dp_15"
    android:paddingRight="@dimen/dp_15">

    <RelativeLayout
        android:id="@+id/item_orderList_delete_ll"
        android:layout_width="@dimen/dp_150"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:paddingBottom="@dimen/dp_10"
        android:paddingTop="@dimen/dp_12">

        <ImageView
            android:id="@+id/item_orderList_delete_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:src="@drawable/shape" />
    </RelativeLayout>

    <TextView
        android:id="@+id/item_orderList_drugName_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_orderList_delete_ll"
        android:text="阿莫西林胶囊阿莫西林阿莫西林西林胶…"
        android:textColor="@color/color_333"
        android:textSize="@dimen/sp_18" />

    <TextView
        android:id="@+id/item_orderList_drugSpec_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_orderList_drugName_tv"
        android:layout_marginTop="@dimen/dp_11"
        android:text="@string/norms"
        android:textColor="@color/color_999"
        android:textSize="@dimen/sp_13" />

    <TextView
        android:id="@+id/item_orderList_specContent_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_orderList_drugName_tv"
        android:layout_marginTop="@dimen/dp_11"
        android:layout_toEndOf="@+id/item_orderList_drugSpec_tv"
        android:text="不详"
        android:textColor="@color/color_999"
        android:textSize="@dimen/sp_13" />

    <TextView
        android:id="@+id/item_orderList_drugPackage_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_orderList_drugSpec_tv"
        android:layout_marginTop="@dimen/dp_8"
        android:text="@string/wrappings"
        android:textColor="@color/color_999"
        android:textSize="@dimen/sp_13" />

    <TextView
        android:id="@+id/item_orderList_packageContent_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_orderList_drugSpec_tv"
        android:layout_marginTop="@dimen/dp_8"
        android:layout_toEndOf="@+id/item_orderList_drugPackage_tv"
        android:text="12粒／盒"
        android:textColor="@color/color_999"
        android:textSize="@dimen/sp_13" />

    <TextView
        android:id="@+id/item_orderList_drugFactory_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_orderList_drugPackage_tv"
        android:layout_marginTop="@dimen/dp_8"
        android:text="@string/factory"
        android:textColor="@color/color_999"
        android:textSize="@dimen/sp_13" />

    <TextView
        android:id="@+id/item_orderList_factoryContent_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_orderList_drugPackage_tv"
        android:layout_marginTop="@dimen/dp_8"
        android:layout_toEndOf="@+id/item_orderList_drugFactory_tv"
        android:text="朗致集团双人药业有限公司"
        android:textColor="@color/color_999"
        android:textSize="@dimen/sp_13" />

    <TextView
        android:id="@+id/item_orderList_drugDate_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_orderList_factoryContent_tv"
        android:layout_marginTop="@dimen/dp_8"
        android:text="@string/validityPeriod"
        android:textColor="@color/color_999"
        android:textSize="@dimen/sp_13" />

    <TextView
        android:id="@+id/item_orderList_dateContent_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_orderList_factoryContent_tv"
        android:layout_marginTop="@dimen/dp_8"
        android:layout_toEndOf="@+id/item_orderList_drugDate_tv"
        android:text="1年"
        android:textColor="@color/color_999"
        android:textSize="@dimen/sp_13" />

    <RelativeLayout
        android:id="@+id/item_orderList_car_rl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_orderList_drugDate_tv"
        android:layout_marginTop="@dimen/dp_10">

        <TextView
            android:id="@+id/item_orderList_moneySign_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/item_orderList_money_tv"
            android:text="@string/moneySign"
            android:textColor="@color/color_EB3E2D"
            android:textSize="@dimen/sp_15" />

        <TextView
            android:id="@+id/item_orderList_money_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/item_orderList_moneySign_tv"
            android:text="188"
            android:textColor="@color/color_EB3E2D"
            android:textSize="@dimen/sp_20" />

        <TextView
            android:id="@+id/item_orderList_initialMoney_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/dp_7"
            android:layout_toEndOf="@+id/item_orderList_money_tv"
            android:text="￥169"
            android:textColor="@color/color_999"
            android:textSize="@dimen/sp_14" />

        <TextView
            android:id="@+id/item_orderList_line"
            android:layout_width="@dimen/dp_39"
            android:layout_height="@dimen/dp_1"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/dp_7"
            android:layout_toEndOf="@+id/item_orderList_money_tv"
            android:background="@color/color_999" />

        <TextView
            android:id="@+id/item_orderList_goodsNum_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toStartOf="@+id/item_orderList_AddGood_iv"
            android:text="10"
            android:textColor="@color/color_333"
            android:textSize="@dimen/sp_18" />

        <ImageView
            android:id="@+id/item_orderList_reduceGood_iv"
            android:layout_width="@dimen/dp_28"
            android:layout_height="@dimen/dp_28"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_10"
            android:layout_toStartOf="@+id/item_orderList_goodsNum_tv"
            android:background="@drawable/white_bg_jian"
            android:padding="@dimen/dp_7"
            android:src="@drawable/jian" />

        <ImageView
            android:id="@+id/item_orderList_AddGood_iv"
            android:layout_width="@dimen/dp_28"
            android:layout_height="@dimen/dp_28"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/dp_10"
            android:background="@drawable/red_bg_num"
            android:padding="@dimen/dp_7"
            android:src="@drawable/add" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/item_orderList_discount_rl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_orderList_car_rl"
        android:layout_marginTop="@dimen/dp_30">

        <TextView
            android:id="@+id/item_orderList_discount_tv"
            android:layout_width="@dimen/dp_36"
            android:layout_height="@dimen/dp_15"
            android:layout_centerVertical="true"
            android:background="@drawable/white_bg_sign"
            android:gravity="center"
            android:text="@string/discount"
            android:textColor="@color/color_EB3E2D"
            android:textSize="@dimen/sp_10" />

        <TextView
            android:id="@+id/item_orderList_discountContent_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_toEndOf="@+id/item_orderList_discount_tv"
            android:text="满30件打9折"
            android:textColor="@color/color_666"
            android:textSize="@dimen/sp_14" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/item_orderList_fullGift_rl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_orderList_discount_rl"
        android:layout_marginTop="@dimen/dp_9">

        <TextView
            android:id="@+id/item_orderList_fullGift_tv"
            android:layout_width="@dimen/dp_36"
            android:layout_height="@dimen/dp_15"
            android:layout_centerVertical="true"
            android:background="@drawable/white_bg_sign"
            android:gravity="center"
            android:text="@string/present"
            android:textColor="@color/color_EB3E2D"
            android:textSize="@dimen/sp_10" />

        <TextView
            android:id="@+id/item_orderList_fullGiftContent_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_toEndOf="@+id/item_orderList_fullGift_tv"
            android:text="赠品」阿莫西林胶囊"
            android:textColor="@color/color_666"
            android:textSize="@dimen/sp_14" />

        <TextView
            android:id="@+id/item_orderList_fullGiftNum_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:text="x30"
            android:textColor="@color/color_333"
            android:textSize="@dimen/sp_14" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/item_orderList_cond_rl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_orderList_fullGift_rl"
        android:layout_marginBottom="@dimen/dp_12"
        android:layout_marginTop="@dimen/dp_9">

        <TextView
            android:id="@+id/item_orderList_cond_tv"
            android:layout_width="@dimen/dp_36"
            android:layout_height="@dimen/dp_15"
            android:layout_centerVertical="true"
            android:background="@drawable/white_bg_sign"
            android:gravity="center"
            android:text="@string/condition"
            android:textColor="@color/color_EB3E2D"
            android:textSize="@dimen/sp_10" />

        <TextView
            android:id="@+id/item_orderList_condContent_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_toEndOf="@+id/item_orderList_cond_tv"
            android:text="满20盒起赠"
            android:textColor="@color/color_666"
            android:textSize="@dimen/sp_14" />
    </RelativeLayout>
</RelativeLayout>
